{
  "sip": {
    "twilio": {
      "detection": {
        "enabled": true,
        "aws_ip_prefixes": [
          "54.", "18.", "52.", "34.", "184.", "3.", "13.", "44.", "35.", "99.",
          "168.86.", "177.71.", "103.", "185.", "208.78.", "67.213."
        ],
        "user_agent_patterns": ["twilio"],
        "header_patterns": ["x-twilio", "twilio"]
      },
      "handshake": {
        "timeout_seconds": 30,
        "expected_events": ["connected", "start"]
      },
      "audio": {
        "sample_rate_in": 8000,
        "sample_rate_out": 8000
      }
    },
    "webhook": {
      "pause_length": 40,
      "template_path": "templates/sip_streams.xml"
    }
  },
  "logging": {
    "level": "INFO",
    "enable_sip_detection_logs": false,
    "enable_handshake_logs": true,
    "enable_transport_logs": true
  }
} 